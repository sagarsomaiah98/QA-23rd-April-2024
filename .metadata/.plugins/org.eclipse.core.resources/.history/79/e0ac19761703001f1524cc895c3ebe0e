
public class Excercise_1 {
	public static void main(String[] args) {
		
		int n1=99;
		int n2=25;
		int n3=30;
		
		System.out.println(n1);
		n2=n1+n3;
		System.out.println(n2);
		n3=n1+n2;
		System.out.println(n3);
		n3=n2*100;
		System.out.println(n1);
		n1=n3*n2;
		System.out.println(n1);
		n3=n1-n2;
		System.out.println(n3);
		n2=n1+n2+n3+n1;
		System.out.println(n2);
		n1=n1-0+n1;
		System.out.println(n1);
		n2=n3+n1;
		System.out.println(n2);
		System.out.println(n3);
		System.out.println(n1);
		n1=n3-n2;
		System.out.println(n1);
		n2=n1;
		System.out.println(n2);
		n3=n1;
		System.out.println(n1);
		
	}

}
