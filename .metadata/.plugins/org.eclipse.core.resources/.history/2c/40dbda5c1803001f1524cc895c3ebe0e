
public class Excercise_2 {

	public static void main(String[] args) {
		double n1=22.10;
		double n2=5.2;
		double n3=10.36;
		
		System.out.println(n1);
		n2=n1+n3;
		System.out.println(n2);
		n3=n1+n2;
		System.out.println(n3);
		n3=n2*100;
		System.out.println(n1);
		n1=n3*n2;
		System.out.println(n1);
		n3=n1-n2;
		System.out.println(n3);
		n2=n1+n2+n3+n1;
		System.out.println(n2);
		n1=n1-0+n1;
		System.out.println(n1);
		n2=n3+n1;
		System.out.println(n2);
		System.out.println(n3);
		System.out.println(n1);
		n1=n3-n2;
		System.out.println(n1);
		n2=n1;
		System.out.println(n2);
		n3=n1;
		System.out.println(n1);
		
	}


	}

}
