
public class MultipleLineComments {
	
	//if you need ignore/comment  multiple lines first select the lines, then press ctrl+shift+/
	//if you need to uncomment multiple lines first select the lines, then press ctrl+shif+\

	public static void main(String[] args) {
		System.out.println("Java is programming language");
		System.out.println("Newyork");
		System.out.println("Sydney");
		
		  System.out.println("Paris"); System.out.println("thailand");
		  System.out.println("mexico");
			/*
			 * System.out.println("Java is programming language");
			 * System.out.println("Newyork"); System.out.println("Sydney");
			 * System.out.println("Paris"); System.out.println("thailand");
			 */
		  System.out.println("mexico");
		  System.out.println("Java is programming language");
		  System.out.println("Newyork"); System.out.println("Sydney");
		  System.out.println("Paris"); System.out.println("thailand");
		 
		System.out.println("mexico");

	}

}
